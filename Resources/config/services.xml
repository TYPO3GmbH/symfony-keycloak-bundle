<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <defaults public="false" />

        <service id="t3g_keycloak.jwtservice" class="T3G\Bundle\Keycload\Service\JWTService">
            <argument type="service" id="Jose\Component\Signature\Serializer\JWSSerializerManagerFactory" />
            <argument type="service" id="jose.jws_verifier.login_typo3_com" />
            <argument type="service" id="jose.key_set.login_typo3_com" />
        </service>
        <service id="T3G\Bundle\Keycload\Service\JWTService" alias="t3g_keycloak.jwtservice" />

    </services>
</container>
